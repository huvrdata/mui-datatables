{
  "name": "mui-datatables",
  "version": "4.3.0",
  "description": "Datatables for React using Material-UI",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --progress --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs:dev": "next docs",
    "docs:build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider next build docs",
    "docs:export": "next export docs -o docs/export",
    "vercel-build": "npm run build && npm run docs:build && next export docs -o docs/out",
    "docs:start": "next start docs",
    "coverage": "jest --coverage",
    "format": "find src/ docs/ test/ -type f -name \"*.js\" ! -path \"*/.next/*\" | xargs prettier --write",
    "format:check": "find src/ docs/ test/ -type f -name \"*.js\" ! -path \"*/.next/*\" | xargs prettier --check",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huvrdata/mui-datatables.git"
  },
  "keywords": [
    "material-ui",
    "datatables",
    "react"
  ],
  "author": "gregnb <gregnb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huvrdata/mui-datatables/issues"
  },
  "homepage": "https://github.com/huvrdata/mui-datatables#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.10.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@lopatnov/rollup-plugin-uglify": "^2.1.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
